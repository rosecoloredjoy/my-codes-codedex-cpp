#include <iostream>

int main() {
  // Write code here
  double month1;
  double month2;

  std::cout << "Digite mês 1:";
  std::cin >> month1;

  std::cout << "Digite mês 2:";
  std::cin >> month2;

  double MoM = (month2 - month1) / month1 * 100;
  std::cout << "\nThe MoM change is " << MoM << "%";
}
